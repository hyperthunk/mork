    <AnnotationAssertion>
        <AnnotationProperty IRI="http://www.w3.org/2004/02/skos/core#example"/>
        <IRI>#referenceDataMapping</IRI>
        <Literal>:Mapping_Contract_LOB rdf:type owl:NamedIndividual , mork:Datum ;
    mork:mappingScheme :Example_Scheme ;
    mork:exactTBoxMatch :Contract ;
